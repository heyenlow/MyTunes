        <Button Content="OK" HorizontalAlignment="Left" Margin="203,165,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.28,0" Click="Button_Click"/>
        <Image x:Name="Pic" HorizontalAlignment="Left" Height="119" Margin="10,10,0,0" VerticalAlignment="Top" Width="198" Source="musicnotes.jpg"/>

    </Grid>
